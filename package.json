{
  "name": "@schematics/update",
  "version": "0.1100.0-next.7+217.a4c50fb",
  "description": "Schematics specific to updating packages",
  "experimental": true,
  "keywords": [
    "angular",
    "Angular CLI",
    "devkit",
    "sdk",
    "Angular DevKit",
    "blueprints",
    "code generation",
    "schematics",
    "schematic"
  ],
  "schematics": "./collection.json",
  "dependencies": {
    "@angular-devkit/core": "github:angular/angular-devkit-core-builds#a4c50fb2a",
    "@angular-devkit/schematics": "github:angular/angular-devkit-schematics-builds#a4c50fb2a",
    "@yarnpkg/lockfile": "1.1.0",
    "ini": "1.3.5",
    "npm-package-arg": "^8.0.0",
    "pacote": "9.5.12",
    "semver": "7.3.2",
    "semver-intersect": "1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/angular-cli.git"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.11.0",
    "yarn": ">= 1.13.0"
  },
  "author": "Angular Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angular/angular-cli/issues"
  },
  "homepage": "https://github.com/angular/angular-cli",
  "husky": {
    "hooks": {
      "commit-msg": "yarn -s ng-dev commit-message pre-commit-validate --file-env-variable HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "yarn -s ng-dev commit-message restore-commit-message-draft --file-env-variable HUSKY_GIT_PARAMS"
    }
  }
